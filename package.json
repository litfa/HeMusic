{
  "name": "he-music",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --port 8080",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "electron": "wait-on tcp:8080 && cross-env NODE_ENV=development electron ./electron/main.ts",
    "electron:serve": "concurrently -k \"yarn dev\" \"yarn electron\"",
    "electron:build": "vite build && electron-builder"
  },
  "dependencies": {
    "@icon-park/vue-next": "^1.3.6",
    "axios": "^0.27.2",
    "element-plus": "^2.1.11",
    "eslint": "^8.15.0",
    "less": "^4.1.2",
    "pinia": "^2.0.14",
    "swiper": "^8.1.4",
    "vue": "^3.2.25",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "electron-builder": "^23.0.3",
    "eslint-plugin-vue": "^8.7.1",
    "lint-staged": "^12.4.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.7",
    "vue-tsc": "^0.34.7",
    "wait-on": "^6.0.1"
  },
  "main": "electron/main.ts",
  "repository": "git@github.com:litfa/HeMusic.git",
  "author": "Litfa <admin@litf.com.cn>",
  "license": "MIT",
  "build": {
    "appId": "com.litf.he-music",
    "productName": "HeMusic",
    "copyright": "Copyright Â© 2022 <litfa>",
    "mac": {
      "category": "public.music.litf"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  }
}